{
  "name": "nodebox-live-localization",
  "version": "0.0.1",
  "author": "@bitcraftlab",
  "license": "MIT",
  "description": "Creating localized versions of nodebox.live",
  "repository": {
    "type": "git",
    "url": "https://github.com/bitcraftlab/nodebox-live-localization"
  },
  "scripts": {
    "extract": "node extract.js g.json ./trans/g_en.trans.json",
    "translate": "node translate.js -EN ./trans/g_en.trans.json -DE ./trans/g_de.trans.json",
    "compile": "node compile.js g.json ./trans/g_de.trans.json ./build/g_de.js"
  },
  "dependencies": {
    "google-translate-noapi": "bitcraftlab/google-translate-noapi"
  },
  "devDependencies": {}
}
